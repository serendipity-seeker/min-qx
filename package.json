{
  "name": "min-qx",
  "version": "2.0.0",
  "main": "src/main.ts",
  "license": "MIT",
  "author": "Neuron99 <neuron99@qubic.org>",
  "scripts": {
    "start": "vite & electron .",
    "dev": "vite .",
    "build": "vite build",
    "package": "electron-builder"
  },
  "build": {
    "appId": "com.example.myapp",
    "files": [
      "dist/**/*",
      "src/**/*",
      "index.html",
      "src/main.ts"
    ],
    "mac": {
      "category": "public.app-category.utilities",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm"
      ]
    }
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.5",
    "@mui/material": "^6.1.5",
    "@qubic-lib/qubic-ts-library": "^0.1.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-iframe": "^1.8.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "electron": "^33.0.2",
    "electron-builder": "^25.1.8",
    "vite": "^5.4.10"
  }
}
